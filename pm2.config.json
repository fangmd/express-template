{
    "apps": {
        "name": "express-template",
        "script": "dist/app.js",
        "watch": true,
        "ignore_watch": [
            "node_modules",
            "logs",
            "uploadFiles"
        ],
        "instances": "2",
        "error_file": "./logs/err.log",
        "out_file": "./logs/out.log",
        "exec_mode": "cluster",
        "max_memory_restart": "500M"
    }
}